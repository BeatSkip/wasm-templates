{
  "$schema": "https://git.demuxedlogic.nl/Beatskip/Wasm-Templates/raw/branch/master/fenix.json",
  "author": "Beatskip",
  "repoName": "Easy WASM Projects",
  "repoUrl": "https://raw.githubusercontent.com/BeatSkip/wasm-templates/master/",
  "version": 1.0,
  "templates": [
    {
      "id": "beatskip-wasm-windows-normal",
      "displayName": "[Emscripten] C++ windows",
      "description": "Create a C++ project structure for EMCC with Makefile",
      "language": "C++",
      "category": [
          "Programming"
      ],
      "img": "https://www.psdmockups.com/wp-content/uploads/2016/07/emscripten.jpg",
      "directories": [
          ".vscode",
          "bin",
          "include",
          "lib",
          "src"
      ],
      "files": {
          "download": [
              {
                  "from": "cpp/main.cpp",
                  "to": "src/main.cpp"
              },
              {
                  "from": "cpp/Makefile",
                  "to": "Makefile"
              },
              {
                  "from": "cpp/tasks.json",
                  "to": ".vscode/tasks.json"
              },
              {
                  "from": "cpp/gdb-launch.json",
                  "to": ".vscode/launch.json"
              }
          ],
          "open": [
              "src/main.cpp"
          ]
      },
      "command": ""
    }
  ]
}
